---
interface Props {
	title: string;
	description: string;
	
}
const { title } = Astro.props;
const { description } = Astro.props;

import "../styles/globals.css";

import Particles from '../components/particles.astro';
import SpeedInsights  from "@vercel/speed-insights/astro"
import Navbar from '../components/Navbar.astro';
import CustomContextMenu from '../components/react/CustomContextMenu';
---

<!DOCTYPE html>
<html lang="en" data-theme="dark">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width" />
		<meta property="og:title" content="vorlie">
        <meta property="og:type" content="website">
        <meta property="og:description" content={description}>
        <meta property="og:image" content="/images/de4515e977f7dce09cbb3bc3fc3924fa.png">
        <meta property="og:url" content="">
        <meta name="description" content={description}>
		<link rel="icon" type="image/png" href="/images/de4515e977f7dce09cbb3bc3fc3924fa-circle.png" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
		<meta name="theme-color" content="#e46876">
		<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
		<link href="https://fonts.googleapis.com/css2?family=Roboto+Condensed:wght@400;700&family=Noto+Sans+JP:wght@400;700&display=swap" rel="stylesheet">
	</head>
	<body>
		<div id="custom-cursor"></div>
		<div id="custom-cursor-outline"></div>
		<SpeedInsights />
		<Particles />
		<Navbar />
		<div class="main-content">
		  	<slot />
		</div> 
		<CustomContextMenu client:load mouseHover />
	  </body>
</html>
<script src="../scripts/lanyard/updateLanyard.ts"></script>
<script src="../scripts/toggleTheme.ts"></script>
<script src="../scripts/cursor.ts"></script>
<script>
import redirect from "../scripts/redirect";

	redirect();
</script>